@using AgendaTelefonica.MVC.Helpers;
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Agenda telefonica</title>

    <link href="@Url.Content("~/Content/css/bootstrap/bootstrap.css")" rel="stylesheet" />
    <link rel="stylesheet" href="http://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.5.0/css/font-awesome.min.css">
    <link rel="stylesheet" href="http://cdnjs.cloudflare.com/ajax/libs/ionicons/2.0.1/css/ionicons.min.css">

    @RenderSection("scriptCss", required: false)
    @Html.PartialRenderScripts(EnumTipoScript.Css)

    <link href="@Url.Content("~/Content/css/dist/AdminLTE.min.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Content/css/dist/skins/_all-skins.min.css")" rel="stylesheet" />
    
</head>
<body class="hold-transition skin-yellow sidebar-mini wysihtml5-supported">

    <div class="wrapper">

        <header class="main-header">
            <a href="@Url.Content("~/Home")" class="logo">
                <span class="logo-mini"><b>MVC</b></span>
                <span class="logo-lg"><b>Template MVC</b></span>
            </a>
            <nav class="navbar navbar-static-top">
                <a href="#" class="sidebar-toggle" data-toggle="offcanvas" role="button">
                    <span class="sr-only">Toggle navigation</span>
                </a>
                <div class="navbar-custom-menu">
                    <ul class="nav navbar-nav">
                        
                    </ul>
                </div>
            </nav>
        </header>

        @Html.Partial("PartialViews/_Menu")
        

        <div class="content-wrapper" style="min-height: 946px;">
            @Html.Partial("PartialViews/_AlertaFeedback")
            @RenderBody()
        </div>

        <footer class="main-footer">
            <div class="pull-right hidden-xs">
                <b>Versão</b> 1.0.0
            </div>
            <strong>Copyright &copy; 2017 <a href="http://www.tegma.com.br">AgendaTelefonica.MVC Gestão Logistica</a>.</strong> Todos os direitos reservados.
        </footer>

        <script src="@Url.Content("~/scripts/plugins/jQuery/jQuery-2.2.0.min.js")"></script>

        <script src="http://code.jquery.com/ui/1.11.4/jquery-ui.min.js"></script>
        <script>
            $.widget.bridge('uibutton', $.ui.button);
        </script>
        <script src="@Url.Content("~/scripts/bootstrap/bootstrap.min.js")"></script>
        <script src="@Url.Content("~/scripts/dist/app.min.js")"></script>
        <script src="@Url.Content("~/scripts/main.js")"></script>
        <script src="@Url.Content("~/scripts/plugins/jQuery/SmartNotification.js")"></script>
        <script type="text/javascript">
            
        </script>

        @RenderSection("scriptJs", required: false)
        @Html.PartialRenderScripts(EnumTipoScript.Javascript)
        </div>
</body>
</html>